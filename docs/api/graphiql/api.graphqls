# 1. create event
mutation {
  createEvent(
    eventInput: {
      title: "title 2"
      description: "desc 2"
      price: 22.22
      date: "2022-10-22T15:14:08"
    }
  ) {
    id
    date
    price
    description
  }
}

# 2. query event: the executing sql must do full column query
query {
  events {
    id
    description
  }
}
